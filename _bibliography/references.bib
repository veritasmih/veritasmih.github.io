doi = {10.1016/j.jada.2008.10.064},
file = {:Users/Ander/Documents/Articles/Briefel, Wilson, Gleason - 2009 - Journal of the American Dietetic Association - Consumption of low-nutrient, energy-dense foods and bev.pdf:pdf},
