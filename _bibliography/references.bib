file = {:Users/Ander/Documents/Articles/Briefel et al. - 2013 - Journal of the Academy of Nutrition and Dietetics - Reducing calories and added sugars by improving children's b.pdf:pdf},
issn = {2212-2672},
journal = {Journal of the Academy of Nutrition and Dietetics},
keywords = {Adolescent,Beverages,Child,Child Nutritional Physiological Phenomena,Child Nutritional Physiological Phenomena: physiol,Cross-Sectional Studies,Diet,Dietary Sucrose,Dietary Sucrose: administration {\&} dosage,Dietary Sucrose: administration {\{}{\&}{\}} dosage,Dietary Sucrose: adverse effects,Dose-Response Relationship,Drug,Energy Intake,Energy Intake: drug effects,Energy Intake: physiology,Female,Food Services,Humans,Male,Nutrition Policy,Obesity,Obesity: epidemiology,Obesity: etiology,Obesity: prevention {\&} control,Obesity: prevention {\{}{\&}{\}} control,Reducing,Schools,Sweetening Agents,Sweetening Agents: administration {\&} dosage,Sweetening Agents: administration {\{}{\&}{\}} dosage,Sweetening Agents: adverse effects,United States,cv},
